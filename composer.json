{
    "name": "cesaredifoggia/melodia",
    "description": "Simple MVC PHP framework: a demonstration of how to create a simple MVC framework in PHP",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Melody\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cesaredifoggiadev/melody.git"
        }
    ],
    "require-dev": {
        "cesaredifoggiadev/melody": "dev-main"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}